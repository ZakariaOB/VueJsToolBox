{
    "name": "pocev-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build-dev": "vue-cli-service build --mode development --watch --dest ../PocEv.Core.Portal/wwwroot/evpoc",
        "lint": "vue-cli-service lint",
        "build-prod": "vue-cli-service build --mode production --watch --dest ../PocEv.Core.Portal/wwwroot/evpoc",
        "test:unit": "jest"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts",
            "json",
            "vue"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "testURL": "http://localhost/",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
    },
    "dependencies": {
        "core-js": "^3.6.4",
        "ejs": "^3.1.5",
        "element-ui": "^2.13.2",
        "loadash": "^1.0.0",
        "lodash": "^4.17.21",
        "rxjs": "^6.6.3",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.1",
        "vue-router": "^3.1.6",
        "vue-rx": "^6.2.0",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@types/jest": "^26.0.19",
        "@types/jquery": "^3.5.2",
        "@types/lodash": "^4.14.167",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "@vue/cli-plugin-babel": "^4.3.0",
        "@vue/cli-plugin-eslint": "^4.3.0",
        "@vue/cli-plugin-router": "^4.3.0",
        "@vue/cli-plugin-typescript": "^4.3.0",
        "@vue/cli-plugin-vuex": "^4.3.0",
        "@vue/cli-service": "^4.3.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.1.2",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.6.3",
        "node-sass": "^4.12.0",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.2",
        "ts-jest": "^26.4.4",
        "typescript": "~3.8.3",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.6.11"
    }
}
